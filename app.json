{
  "name": "Quickstart Andro",
  "description": "A simple Python server to distribute access tokens and route calls for your mobile app",
  "repository": "https://github.com/androstudio/andro-studio-team",
  "logo": "https://s-media-cache-ak0.pinimg.com/600x315/a2/19/e1/a219e1302acd9caf8949f7efb180a75e.jpg",
  "keywords": [
    "twilio",
    "voice",
    "mobile"
  ],
  "env": {
    "ACCOUNT_SID": {
      "description": "Your Twilio Account Sid",
      "required": true
    },
    "API_KEY": {
      "description": "Your Twilio API Key",
      "required": true
    },
    "API_KEY_SECRET": {
      "description": "Your Twilio API Key Secret",
      "required": true
    },
    "APP_SID": {
      "description": "Your TwiML App Sid that points to this web server",
      "required": true
    },
    "AUTH_TOKEN": {
      "description": "Your Twilio Auth Token",
      "required": true
    },
    "PUSH_CREDENTIAL_SID": {
      "description": "Your Twilio Push Credential Sid",
      "required": true
    }
  }
}
